<form method="post" action="{{ path('app_admin_shopping_list_shopping_list_delete', {'id': shopping_list.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ shopping_list.id) }}">
    <button class="btn btn-danger"><i class="fas fa-trash"></i> Delete</button>
</form>
